<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.longhorn.dvrexplorer.DVRActivity">

    <android.support.v4.view.ViewPager
        android:id="@+id/file_pho_viewpager"
        android:layout_width="860px"
        android:layout_height="483px"
        android:layout_marginLeft="6px"
        android:layout_marginTop="32px" />

    <Button
        android:id="@+id/file_pho_return"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="30px"
        android:layout_alignParentBottom="true"
        android:background="@drawable/file_pho_return"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_left"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="265px"
        android:layout_alignParentBottom="true"
        android:background="@drawable/file_pho_left"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_right"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="498px"
        android:layout_alignParentBottom="true"
        android:background="@drawable/file_pho_right"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_del"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="633px"
        android:layout_alignParentBottom="true"
        android:background="@drawable/file_pho_del"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_share"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="780px"
        android:layout_alignParentBottom="true"
        android:background="@drawable/file_pho_share"
        android:enabled="false" />
</RelativeLayout>
