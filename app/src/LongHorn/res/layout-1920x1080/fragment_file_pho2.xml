<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="-146px"
    android:layout_marginTop="-106px"
    tools:context="com.longhorn.dvrexplorer.DVRActivity">

    <android.support.v4.view.ViewPager
        android:id="@+id/file_pho_viewpager"
        android:layout_width="1182px"
        android:layout_height="663px"
        android:layout_marginLeft="477px"
        android:layout_marginTop="199px" />

    <Button
        android:id="@+id/file_pho_return"
        android:layout_width="147px"
        android:layout_height="147px"
        android:layout_marginLeft="435px"
        android:layout_marginTop="897px"
        android:background="@drawable/file_pho_return"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_left"
        android:layout_width="147px"
        android:layout_height="147px"
        android:layout_marginLeft="787px"
        android:layout_marginTop="897px"
        android:background="@drawable/file_pho_left"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_right"
        android:layout_width="147px"
        android:layout_height="147px"
        android:layout_marginLeft="1137px"
        android:layout_marginTop="897px"
        android:background="@drawable/file_pho_right"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_del"
        android:layout_width="147px"
        android:layout_height="147px"
        android:layout_marginLeft="1348px"
        android:layout_marginTop="897px"
        android:background="@drawable/file_pho_del"
        android:enabled="true" />

    <Button
        android:id="@+id/file_pho_share"
        android:layout_width="147px"
        android:layout_height="147px"
        android:layout_marginLeft="1560px"
        android:layout_marginTop="897px"
        android:background="@drawable/file_pho_share"
        android:enabled="false" />
</RelativeLayout>
