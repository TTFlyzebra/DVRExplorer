<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="-146px"
    android:layout_marginTop="-106px"
    tools:context="com.longhorn.dvrexplorer.DVRActivity">

    <tcking.github.com.giraffeplayer.IjkVideoView
        android:id="@+id/file_video_vv"
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px" />
    <LinearLayout
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/fragment_video_tv07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#000"
            android:textSize="24px"
            android:drawableBottom="@drawable/icon_forward"
            android:gravity="center"
            android:visibility="gone" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:id="@+id/fragment_video_tv08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#000"
            android:textSize="24px"
            android:drawableBottom="@drawable/icon_backward"
            android:gravity="center"
            android:visibility="gone" />
    </LinearLayout>

    <SeekBar
        android:id="@+id/file_video_sk"
        android:layout_width="478px"
        android:layout_height="10px"
        android:layout_marginLeft="473px"
        android:layout_marginTop="536px"
        android:thumb="@android:color/transparent" />

    <TextView
        android:id="@+id/file_video_sktv1"
        android:layout_width="80px"
        android:layout_height="24px"
        android:layout_marginLeft="388px"
        android:layout_marginTop="524px"
        android:gravity="right"
        android:textColor="#FFF"
        android:textSize="22px" />

    <TextView
        android:id="@+id/file_video_sktv2"
        android:layout_width="80px"
        android:layout_height="24px"
        android:layout_marginLeft="956px"
        android:layout_marginTop="524px"
        android:gravity="left"
        android:textColor="#FFF"
        android:textSize="22px" />

    <ImageView
        android:id="@+id/file_video_iv"
        android:layout_width="788px"
        android:layout_height="442px"
        android:layout_marginLeft="318px"
        android:layout_marginTop="133px" />

    <Button
        android:id="@+id/file_pho_return"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="290px"
        android:layout_marginTop="598px"
        android:background="@drawable/file_pho_return"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_left"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="525px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_left"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_play"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="662px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_play"
        android:enabled="true" />

    <Button
        android:id="@+id/file_video_right"
        android:layout_width="98px"
        android:layout_height="98px"
        android:layout_marginLeft="798px"
        android:layout_marginTop="602px"
        android:background="@drawable/file_video_right"
        android:enabled="true" />
</RelativeLayout>
